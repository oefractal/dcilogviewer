<html>
<head>
  <title>Просмотрщик лог-файлов</title>
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jqgrid")
    @Scripts.Render("~/bundles/jqueryui")
    @Styles.Render("~/Content/jqueryui")
    @Styles.Render("~/Content/jqgrid")
</head>
<body>
  <div id="file-name-input-and-label">
    <div id="file-name-label-div">
      <span id="file-name-label">Имя файла:</span>
    </div>
    <div id="file-name-input-div">
      <input id="file-name-input" />
    </div>
    <div id="file-name-button-div">
      <button id="select-file-button">Выбрать</button>
    </div>
  </div>
  <div id="bottom-frame">
    <div id="log-view-grid-container">
      <table id="log-view-grid"></table>
      <div id="log-view-grid-pager"></div>
    </div>
    <div id="stack-input-div">
      <textarea id="stack-input"></textarea>
    </div>
  </div>  
  @Scripts.Render("~/bundles/index")
</body>
</html>